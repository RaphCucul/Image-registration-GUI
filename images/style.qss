QPushButton {
	background-color: white;
	border-style: outset;
        border-width: 1px;
        border-radius: 2px;
        border-color: grey;
        font: 11px;
	padding: 2px;
}
QPushButton:pressed {
    border-style: inset;
    border-color: blue;
    border-width: 3px;
    color: white;
}
QPushButton:hover {
    background-color: qlineargradient(x1: 0, y1: 0, x2: 0, y2: 1,stop: 0 #109910, stop: 1 #00ff00);
    border-color: #66ff66;
}
QComboBox {
    border: 1px solid black;
    border-radius: 2px;
    padding: 1px 18px 1px 3px;
    min-width: 6em;
}
QComboBox QAbstractItemView {
    border: 2px solid darkgray;
    selection-background-color: lightgray;
}
QProgressBar {
    border: 2px solid grey;
    border-radius: 5px;
    text-align: center;
}
QProgressBar::chunk{
    background-color: #33aa00;
    width: 20px;
}
QLineEdit {
    border: 1px solid #f7f8f9;
    border-radius: 2px;
    padding: 2px;
    max-height: 14px;
}
